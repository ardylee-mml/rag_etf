  "exampleQueries": [
    {
      "description": "Find most visited zones",
      "question": "Which zones have been visited the most?",
      "pipeline": [
        {
          "$lookup": {
            "from": "events",
            "localField": "_id",
            "foreignField": "context.zoneId",
            "as": "visits"
          }
        },
        {
          "$project": {
            "_id": 1,
            "name": 1,
            "visitCount": {
              "$size": "$visits"
            }
          }
        },
        {
          "$sort": {
            "visitCount": -1
          }
        },
        {
          "$limit": 10
        }
      ]
    },
    {
      "description": "Count total documents",
      "question": "How many documents are in the zones collection?",
      "pipeline": [
        {
          "$count": "total"
        }
      ]
    }
  ]
